    "# recursive function using unpack operator for suming the digits of the number\n",
    "def sum(items: List[int]) -> int:\n",
    "    head, *tail = items\n",
    "    if tail:\n",
    "        return head + sum(tail)\n",
    "    else:\n",
    "        return head\n",
    "\n",
